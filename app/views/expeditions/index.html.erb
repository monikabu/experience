<p id="notice"><%= notice %></p>

<h1>Listing Expeditions</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Starting point</th>
      <th>Destination</th>
      <th>Start date</th>
      <th>End datedatetime</th>
      <th>Coment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expeditions.each do |expedition| %>
      <tr>
        <td><%= expedition.title %></td>
        <td><%= expedition.starting_point %></td>
        <td><%= expedition.destination %></td>
        <td><%= expedition.start_date %></td>
        <td><%= expedition.end_datedatetime %></td>
        <td><%= expedition.coment %></td>
        <td><%= link_to 'Show', expedition %></td>
        <td><%= link_to 'Edit', edit_expedition_path(expedition) %></td>
        <td><%= link_to 'Destroy', expedition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Expedition', new_expedition_path %>
